		<Element Type="SqlProcedure" Name="[dbo].[UspGetUserByEmail]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
    SET NOCOUNT, XACT_ABORT ON;
    BEGIN TRY
        SELECT 
            [UserId],
            [FirstName],
            [LastName],
            [Email],
            [Password],
            [CreatedAt]
        FROM 
            [dbo].[User]
        WHERE
            [Email] = @Email
    END TRY
    BEGIN CATCH
        THROW
    END CATCH
END]]></Value>
